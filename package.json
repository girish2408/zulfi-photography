{
  "name": "zulfi-photographer",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:railway": "ls -la && turbo run build --filter=web...",
    "start:railway": "cd apps/web && npm start",
    "lint": "turbo run lint",
    "db:push": "npm run db:push --workspace=packages/db",
    "db:studio": "npm run db:studio --workspace=packages/db"
  },
  "dependencies": {
    "turbo": "latest"
  },
  "devDependencies": {
    "prettier": "latest"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "npm@10.2.4"
}
